{
	"compilerOptions": {
	  "target": "es2015",
	  "lib": ["es2015", "dom"],
	  "module": "commonjs",
	  "strict": true,
	  "esModuleInterop": true,
	  "jsx": "react",
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "noEmit": true
	},
	"exclude": ["node_modules"]
  }
  